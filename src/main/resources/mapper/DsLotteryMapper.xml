<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.songbo.dicshop.mapper.DsLotteryMapper">

    <select id="getListByStatus" resultType="com.songbo.dicshop.entity.DsLottery">
        select *
        from ds_lottery
        where ds_lottery_status = #{status}
    </select>
</mapper>