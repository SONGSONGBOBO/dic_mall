<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.songbo.dicshop.mapper.DsAuctionAutoMapper">

    <select id="getListByStatus" resultType="com.songbo.dicshop.entity.DsAuctionAuto">
